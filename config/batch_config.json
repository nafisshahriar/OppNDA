{
  "folder": "reportQP",
  "file_filter": {
    "extension": ".txt",
    "contains": ""
  },
  "report_types": [
    "CCNApplicationReport",
    "MessageStatsReport"
  ],
  "filename_pattern": {
    "delimiter": "_",
    "components": {
      "prefix": 0,
      "router": 1,
      "seed": 2,
      "ttl": 3,
      "buffer": 4,
      "distro": 5,
      "report_type": 6
    },
    "extract": {
      "buffer": "(\\d+)"
    }
  },
  "data_separator": ":",
  "ignore_fields": [
    "sim_time"
  ],
  "average_groups": [],
  "output": {
    "precision": 4
  }
}